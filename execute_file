#include "main.h"

void execute_file(char *file_path) {

    int pid = fork();

    if (pid < 0) {
        perror("Error");
        return;
    }

    if (pid == 0) {
        char *argv[] = { file_path, NULL };
        execvp(file_path, argv);
    } else {
        int status;
        waitpid(pid, &status, 0);
    }
}
